    "next-seo": "^5.4.0",
    "notistack": "^2.0.8",
    "openai": "^6.1.0",
    "optics-ts": "^2.4.0",
    "qrcode.react": "^3.0.2",
    "qs": "^6.11.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^3.1.4",
    "react-intersection-observer": "^9.8.0",
    "react-intl": "^5.24.8",
    "react-qr-code": "^2.0.15",
    "react-resizable": "^3.0.4",
    "react-ts-tradingview-widgets": "^1.2.8",
