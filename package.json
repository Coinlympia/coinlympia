{
  "name": "coinlympia",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": [
    "frontend",
    "backend",
    "frontend/packages/*"
  ],
  "scripts": {
    "postinstall": "yarn build:packages || true",
    "build": "yarn workspace coinlympia-frontend build",
    "build:packages": "yarn workspace @dexkit/dexappbuilder-render run build",
    "dev": "yarn workspace coinlympia-frontend dev",
    "dev:backend": "yarn workspace coinlympia-backend dev",
    "dev:all": "concurrently \"yarn dev\" \"yarn dev:backend\"",
    "start": "yarn workspace coinlympia-frontend start",
    "lint": "yarn workspace coinlympia-frontend lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "dotenv -e backend/.env -- prisma generate --schema=backend/prisma/schema.prisma",
    "db:push": "dotenv -e backend/.env -- prisma db push --schema=backend/prisma/schema.prisma",
    "db:migrate": "dotenv -e backend/.env -- prisma migrate dev --schema=backend/prisma/schema.prisma",
    "db:studio": "dotenv -e backend/.env -- prisma studio --schema=backend/prisma/schema.prisma --browser none",
    "db:seed": "dotenv -e backend/.env -- tsx backend/prisma/seed.ts",
    "db:reset": "dotenv -e backend/.env -- tsx backend/scripts/reset-database.ts",
    "db:fix-duplicates": "dotenv -e backend/.env -- tsx backend/scripts/fix-duplicate-users.ts",
    "update-prices": "dotenv -e backend/.env -- tsx frontend/scripts/run-price-update.ts"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@formatjs/cli": "^4.8.3",
    "@prisma/client": "^6.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cookie": "^0.5.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/eth-url-parser": "^1.0.3",
    "@types/merge-images": "^1.2.4",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20",
    "@types/qs": "^6.9.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-resizable": "^3.0.3",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/react-window": "^1.8.5",
    "@types/sharp": "^0.31.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-custom": "*",
    "eslint-config-next": "15.5.5",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "next-transpile-modules": "^10.0.0",
    "pre-push": "^0.1.4",
    "prettier": "latest",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.6",
    "turbo": "^1.11.3",
    "typescript": "5.8.3"
  },
  "pre-push": [
    "build"
  ],
  "browser": {
    "child_process": false,
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "dgram": false,
    "module": false
  },
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@0x/contract-addresses": "^8.13.0",
    "@apollo/client": "^3.7.0",
    "@dexkit/core": "*",
    "@dexkit/dexappbuilder-viewer": "*",
